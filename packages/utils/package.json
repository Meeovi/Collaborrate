{
	"name": "@alternatecms/utils",
	"version": "9.22.4",
	"type": "module",
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"description": "Utilities shared between the Alternatecms packages",
	"repository": {
		"type": "git",
		"url": "https://github.com/alternatecms/alternatecms.git",
		"directory": "packages/utils"
	},
	"funding": "https://github.com/alternatecms/alternatecms?sponsor=1",
	"license": "GPL-3.0",
	"author": "Rijk van Zanten <rijkvanzanten@me.com>",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./node": {
			"types": "./dist/node/index.d.ts",
			"import": "./dist/node/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"files": [
		"dist",
		"!**/*.d.ts?(.map)"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@alternatecms/shared": "workspace:*",
		"@alternatecms/storage": "workspace:*"
	},
	"devDependencies": {
		"@alternatecms/tsconfig": "0.0.6",
		"@ngneat/falso": "6.4.0",
		"@vitest/coverage-c8": "0.28.5",
		"typescript": "4.9.5",
		"vitest": "0.28.5"
	}
}
