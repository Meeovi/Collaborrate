model Idempotency {
  objectId  String    @id
  createdAt DateTime? @db.Timestamptz(6)
  updatedAt DateTime? @db.Timestamptz(6)
  reqId     String?   @unique(map: "_Idempotency.reqId_unique")
  expire    DateTime? @db.Timestamptz(6)
  rperm     String[]  @map("_rperm")
  wperm     String[]  @map("_wperm")

  @@index([expire], map: "_Idempotency.expire_index")
  @@map("_Idempotency")
}

model JobSchedule {
  objectId      String    @id
  createdAt     DateTime? @db.Timestamptz(6)
  updatedAt     DateTime? @db.Timestamptz(6)
  jobName       String?
  description   String?
  params        String?
  startAfter    String?
  daysOfWeek    Json?
  timeOfDay     String?
  lastRun       Float?
  repeatMinutes Float?
  rperm         String[]  @map("_rperm")
  wperm         String[]  @map("_wperm")

  @@map("_JobSchedule")
}

model JobStatus {
  objectId   String    @id
  createdAt  DateTime? @db.Timestamptz(6)
  updatedAt  DateTime? @db.Timestamptz(6)
  jobName    String?
  source     String?
  status     String?
  message    String?
  params     Json?
  finishedAt DateTime? @db.Timestamptz(6)
  rperm      String[]  @map("_rperm")
  wperm      String[]  @map("_wperm")

  @@map("_JobStatus")
}

model Join_roles__Role {
  relatedId String @db.VarChar(120)
  owningId  String @db.VarChar(120)

  @@id([relatedId, owningId])
  @@map("_Join:roles:_Role")
}

model Join_users__Role {
  relatedId String @db.VarChar(120)
  owningId  String @db.VarChar(120)

  @@id([relatedId, owningId])
  @@map("_Join:users:_Role")
}

model PushStatus {
  objectId            String    @id
  createdAt           DateTime? @db.Timestamptz(6)
  updatedAt           DateTime? @db.Timestamptz(6)
  pushTime            String?
  source              String?
  query               String?
  payload             String?
  title               String?
  expiry              Float?
  expiration_interval Float?
  status              String?
  numSent             Float?
  numFailed           Float?
  pushHash            String?
  errorMessage        Json?
  sentPerType         Json?
  failedPerType       Json?
  sentPerUTCOffset    Json?
  failedPerUTCOffset  Json?
  count               Float?
  rperm               String[]  @map("_rperm")
  wperm               String[]  @map("_wperm")

  @@map("_PushStatus")
}

model Role {
  objectId  String    @id
  createdAt DateTime? @db.Timestamptz(6)
  updatedAt DateTime? @db.Timestamptz(6)
  name      String?   @unique(map: "_Role.name_unique")
  rperm     String[]  @map("_rperm")
  wperm     String[]  @map("_wperm")

  @@map("_Role")
}

model User {
  objectId                      String    @id
  createdAt                     DateTime? @db.Timestamptz(6)
  updatedAt                     DateTime? @db.Timestamptz(6)
  username                      String?   @unique(map: "_User.username_unique")
  email                         String?   @unique(map: "_User.email_unique")
  emailVerified                 Boolean?
  authData                      Json?
  rperm                         String[]  @map("_rperm")
  wperm                         String[]  @map("_wperm")
  hashed_password               String?   @map("_hashed_password")
  email_verify_token_expires_at DateTime? @map("_email_verify_token_expires_at") @db.Timestamptz(6)
  email_verify_token            String?   @map("_email_verify_token")
  account_lockout_expires_at    DateTime? @map("_account_lockout_expires_at") @db.Timestamptz(6)
  failed_login_count            Float?    @map("_failed_login_count")
  perishable_token              String?   @map("_perishable_token")
  perishable_token_expires_at   DateTime? @map("_perishable_token_expires_at") @db.Timestamptz(6)
  password_changed_at           DateTime? @map("_password_changed_at") @db.Timestamptz(6)
  password_history              Json?     @map("_password_history")

  @@map("_User")
}

model core_store {
  id          Int     @id @default(autoincrement())
  key         String? @db.VarChar(255)
  value       String?
  type        String? @db.VarChar(255)
  environment String? @db.VarChar(255)
  tag         String? @db.VarChar(255)
}

model polls {
  id          Int       @id @default(autoincrement())
  name        String
  excerpt     String?
  description String?
  image       String?
  published   DateTime? @default(now()) @db.Timestamp(6)
  question    String?
  response    String?
  categories  String?
  articles    String?
  products    String?
  customers   String?
  users       String?
}

model product_attribute_set {
  id                Int     @id @default(autoincrement())
  name              String  @db.VarChar(255)
  based_on          String?
  attribute_id      Int     @default(autoincrement())
  product_attribute String?
}

model roles {
  id         Int       @id @default(autoincrement())
  role_name  String
  content    String?
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}

model source {
  id              Int      @id @default(autoincrement())
  name            String   @db.VarChar(255)
  code            Int      @default(autoincrement())
  enabled         Boolean?
  description     String?  @db.VarChar(255)
  latitude        String?
  longitude       String?
  pickup_location Boolean?
  contact_name    String?  @db.VarChar(255)
  email           String?  @db.VarChar(255)
  phone           Decimal? @db.Decimal
  fax             Decimal? @db.Decimal
  country         String?
  state           String?
  city            String?
  street          String?
  postcode        Decimal? @db.Decimal
}

model tasks {
  id            BigInt    @id @default(autoincrement())
  created_at    DateTime? @default(now()) @db.Timestamptz(6)
  subject       String?
  start_date    String?
  due_date      String?
  priority      String?
  description   String?
  status        String?
  related_to    String?
  contact       String?
  assigned_to   String?
  date_modified DateTime? @default(now()) @db.Timestamptz(6)
  mediamanager  String?
  projects      String?
  workspaces    String?
}

model upload_file {
  id                Int       @id @default(autoincrement())
  name              String    @db.VarChar(255)
  alternativeText   String?   @db.VarChar(255)
  caption           String?   @db.VarChar(255)
  width             Int?
  height            Int?
  formats           Json?
  hash              String    @db.VarChar(255)
  ext               String?   @db.VarChar(255)
  mime              String    @db.VarChar(255)
  size              Decimal   @db.Decimal(10, 2)
  url               String    @db.VarChar(255)
  previewUrl        String?   @db.VarChar(255)
  provider          String    @db.VarChar(255)
  provider_metadata Json?
  created_by        Int?
  updated_by        Int?
  created_at        DateTime? @default(now()) @db.Timestamptz(6)
  updated_at        DateTime? @default(now()) @db.Timestamptz(6)
}

model upload_file_morph {
  id             Int     @id @default(autoincrement())
  upload_file_id Int?
  related_id     Int?
  related_type   String?
  field          String?
  order          Int?
}
