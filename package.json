{
  "name": "alternatecms",
  "version": "0.0.3",
  "description": "A headless content management system with ecommerce features, graphql, Nuxtjs, and Prisma ORM.",
  "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
  "homepage": "https://www.alternatecms.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bastianhilton/AlternateCMS.git"
  },
  "bugs": {
    "url": "https://github.com/bastianhilton/AlternateCMS/issues"
  },
  "license": "MIT",
  "private": true,
  "keywords": [
    "Shopping cart",
    "express",
    "mongodb",
    "nodejs",
    "ecommerce",
    "paypal",
    "stripe",
    "authorize.net",
    "adyen",
    "lunr",
    "cart",
    "shopping",
    "cms",
    "prisma"
  ],
  "scripts": {
    "dev": "nuxt && node server/server.js",
    "start": "nuxt start && NODE_ENV=production node server/server.js",
    "build": "nuxt build",
    "mesh": "npx graphql-mesh dev --port 4002",
    "mesh-start": "mesh start",
    "mesh-build": "mesh build",
    "generate": "nuxt generate",
    "prisma-dev": "npx prisma generate",
    "db-dev": "prisma migrate dev",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test:e2e": "nightwatch",
    "postgraphile": "dotenv cross-var npx postgraphile -c %DASHBOARD_URL% --watch --enhance-graphiql --dynamic-json --owner-connection",
    "test": "echo \"Error: no test specified\" && exit 1",
    "all": "npm-run-all --parallel dev prisma-dev mesh"
  },
  "dependencies": {
    "@graphql-mesh/cli": "^0.73.3",
    "@graphql-mesh/postgraphile": "^0.20.30",
    "@nuxt/http": "^0.6.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.33.0",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/sentry": "^5.1.6",
    "@sentry/tracing": "^6.16.1",
    "@sentry/vue": "^6.16.1",
    "@tinymce/tinymce-vue": "^3.2.8",
    "ap-style-title-case": "^1.1.2",
    "apollo-server": "^3.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "core-js": "^3.24.0",
    "cross-var": "^1.1.0",
    "datatables.net": "^1.12.1",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "dropzone": "^6.0.0-beta.2",
    "eslint": "^8.20.0",
    "formidable": "^2.0.1",
    "formiojs": "^4.14.8",
    "graphql-tag": "^2.12.6",
    "helmet": "^5.0.1",
    "knex": "^2.2.0",
    "knex-schema-inspector": "^2.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.5",
    "nuxt": "^2.15.8",
    "nuxt-client-init-module": "^0.3.0",
    "nuxt-highcharts": "^2.0.4",
    "nuxt-stripe-module": "^3.2.0",
    "oracledb": "^5.4.0",
    "postgraphile-apollo-server": "^0.1.1",
    "sqlite3": "^5.0.10",
    "stripe": "^8.184.0",
    "tedious": "^15.0.0",
    "tinymce": "^6.1.0",
    "webpack": "^4.45.0"
  },
  "devDependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "@prisma/client": "^4.1.0",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "nightwatch": "^1.7.6",
    "npm-run-all": "^4.1.5",
    "nuxt-graphql-request": "^5.2.0",
    "pg": "^8.7.3",
    "prettier": "^2.3.2",
    "prisma": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=8.13.2",
    "node": ">=16.14.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
